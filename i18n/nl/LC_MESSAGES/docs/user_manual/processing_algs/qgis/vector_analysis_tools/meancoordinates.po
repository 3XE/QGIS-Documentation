#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:6
msgid "Mean coordinate(s)"
msgstr "Gemiddelde coördina(a)t(en)"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:9
msgid "Description"
msgstr "Omschrijving"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:11
msgid ""
"Calculates the mean of the coordinates of a layer starting from a field of "
"the attribute table."
msgstr "Berekent het gemiddelde van de coördinaten van een laag, beginnend vanuit een veld van de attributentabel."

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:15
msgid "Parameters"
msgstr "Parameters"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:18
msgid "``Input layer`` [vector: any]"
msgstr "``Invoerlaag`` [vector: elke]"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:18
msgid "<put parameter description here>"
msgstr "<plaats omschrijving parameter hier>"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:23
msgid "``Weight field`` [tablefield: numeric]"
msgstr "``Gewogen veld`` [tabelveld: numeriek]"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:21
#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:26
msgid "Optional."
msgstr "Optioneel."

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:23
msgid "Field to use if you want to perform a weighted mean."
msgstr "Te gebruiken veld als u een gewogen gemiddelde wilt uitvoeren."

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:28
msgid "``Unique ID field`` [tablefield: numeric]"
msgstr "``Uniek veld ID`` [tabelveld: numeriek]"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:28
msgid "Unique field on which the calculation of the mean will be made."
msgstr "Uniek veld waarop het berekenen van het gemiddelde zal worden uitgevoerd."

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:31
msgid "Outputs"
msgstr "Uitvoer"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:34
msgid "``Result`` [vector]"
msgstr "``Resultaat`` [vector]"

#: ../../source/docs/user_manual/processing_algs/qgis/vector_analysis_tools/meancoordinates.rst:34
msgid "The resulting points layer."
msgstr "De resulterende polygonenlaag."

