msgid "Buffer distance for \"fixed value\" method."
msgstr "Distance tampon pour la méthode \"valeurs fixes\"."

#: ../../source/docs/user_manual/processing_algs/saga/shapes_tools/shapesbuffer.rst:32
msgid "Default: *100.0*"
msgstr "Par défaut : *100.0*"

